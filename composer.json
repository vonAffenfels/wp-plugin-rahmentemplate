{
  "name": "va/wp-plugin-rahmentemplate",
  "type": "wordpress-plugin",
  "description": "Plugin to replace content from a Template",
  "homepage": "https://github.com/vonAffenfels/wp-plugin-rahmentemplate",
  "authors": [
    {
      "name": "Charalambos Schmechel",
      "email": "charalambos.schmechel@vonaffenfels.de",
      "homepage": "https://www.vonaffenfels.de"
    }
  ],
  "keywords": [
    "php",
    "wordpress"
  ],
  "autoload": {
    "psr-4": {
      "Rahmentemplate\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.4 | ^7.4"
  },
  "scripts": {
    "test": [
      "phpcs"
    ]
  }
}