{
  "name": "va/wp-plugin-rahmentemplate",
  "type": "wordpress-plugin",
  "description": "Plugin to replace content from a Template",
  "homepage": "https://github.com/vonAffenfels/wp-plugin-rahmentemplate",
  "authors": [
    {
      "name": "Charalambos Schmechel",
      "email": "charalambos.schmechel@vonaffenfels.de",
      "homepage": "https://www.vonaffenfels.de"
    }
  ],
  "support": {
    "issues": "https://github.com/vonAffenfels/wp-plugin-rahmentemplate/issues"
  },
  "keywords": [
    "wordpress",
    "wp",
    "plugin",
    "template",
    "replace"
  ],
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:vonAffenfels/vaf-wp-framework.git"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "WP\\Plugin\\Rahmentemplate\\": "src"
    }
  },
  "scripts": {

  },

  "require-dev": {
    "php": ">=8.1",
    "aws/aws-sdk-php": "^3.297",
    "guzzlehttp/guzzle": "^7.0",
    "humbug/php-scoper": "^0.18.3",
    "php-http/guzzle7-adapter": "^1.0",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.6",
    "vonaffenfels/vaf-wp-framework": "^0.2"
  }
}